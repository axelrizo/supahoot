{
  "name": "supahoot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "supabase:generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > ./database.types.ts",
    "ci:lint": "eslint .",
    "ci:format": "prettier --check src/",
    "ci:type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.2",
    "@dicebear/converter": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@supabase/supabase-js": "^2.48.1",
    "@tailwindcss/vite": "^4.0.3",
    "qrcode.vue": "^3.6.0",
    "tailwindcss": "^4.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "daisyui": "^5.0.0-beta.6",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^17.0.2",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "onchange": "^7.1.0",
    "prettier": "^3.4.2",
    "supabase": "^2.19.6",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^3.0.2",
    "vue-router-mock": "^1.1.0",
    "vue-tsc": "^2.2.0"
  }
}
